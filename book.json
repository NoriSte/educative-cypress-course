{
  "output": "docs",
  "plugins": [
    "include-codeblock",
    "-sharing",
    "include",
    "copy-code-button",
    "github",
    "sitemap",
    "ga",
    "scripts",
    "anchors",
    "head-append"
  ],
  "links": {
    "sidebar": {
      "ReactJSDay 2019": "https://2019.reactjsday.it"
    },
    "sharing": {}
  },
  "styles": {
    "website": "book/styles/website.css"
  },
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/NoriSte/reactjsday-2019-testing-course"
    },
    "sitemap": {
      "hostname": "https://noriste.github.io/reactjsday-2019-testing-course/"
    },
    "ga": {
      "token": "UA-142637808-2"
    },
    "scripts": {
      "files": ["utils/ga-anonymize-ip.js"]
    },
    "head-append": {
      "code": [
        "<link rel=\"apple-touch-icon\" sizes=\"180x180\" href=\"/assets/favicons/apple-touch-icon.png\">",
        "<link rel=\"icon\" type=\"image/png\" sizes=\"32x32\" href=\"/assets/favicons/favicon-32x32.png\">",
        "<link rel=\"icon\" type=\"image/png\" sizes=\"16x16\" href=\"/assets/favicons/favicon-16x16.png\">",
        "<link rel=\"manifest\" href=\"/assets/favicons/site.webmanifest\">",
        "<link rel=\"mask-icon\" href=\"/assets/favicons/safari-pinned-tab.svg\" color=\"#50ade5\">",
        "<link rel=\"shortcut icon\" href=\"/assets/favicons/favicon.ico\">",
        "<meta name=\"msapplication-TileColor\" content=\"#ffffff\">",
        "<meta name=\"msapplication-config\" content=\"/assets/favicons/browserconfig.xml\">",
        "<meta name=\"theme-color\" content=\"#50ade5\">"
      ]
    }
  }
}
